{
  "name": "f12-open-file",
  "displayName": "F12: Open File",
  "description":
    "Extending Typescript Go to definition to open file when the look up symbol is a file name with relative path.",
  "version": "0.1.3",
  "publisher": "rexebin",
  "repository": {
    "url": "https://github.com/rexebin/F12-Open-File"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "icon": "f12.png",
  "categories": ["Languages", "Other"],
  "activationEvents": ["*"],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "title": "Side by side",
      "properties": {
        "openFile.openSideBySide": {
          "type": "boolean",
          "default": true,
          "description": "Open files side by side to the Typescript file."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
